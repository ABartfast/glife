     if DrunkDPTrand<50:
     DPtipe=1
     DPgrup=2
     $din_disc_dance='It suits you the guy dressed in sweatpants and sweatshirts same and offers dance.'
     $din_disc_dance2='Near you dancing guy in sweatpants and sweatshirts.'
     end
     if DrunkDPTrand>=50:
     DPtipe=2
     DPgrup=2
     $din_disc_dance='It suits you drunk guy dressed in sweatpants and sweatshirts same and takes you arms around his waist offering dance.'
     $din_disc_dance2='Near you dancing drunk guy in sweatpants and sweatshirts.'
     end
end
if DaPaTipeRand>=90:
!traveling_expenses
