$tracklist_club[3] = 'music/club3.mp3'
$tracklist_club[4] = 'music/club4.mp3'
$tracklist_fight[0] = 'music/fight.mp3'
$tracklist_fight[1] = 'music/fight1.mp3'
$tracklist_orochimaru[0] = 'music/orochimaru.mp3'

! WD: Uninitialised ~ 'club'
if club = 1:
	clubrand = rand(0, 5)

	if clubrand = 0:
		$tracklist_club[0] = 'music/club.mp3'
	elseif clubrand = 1:
		$tracklist_club[0] = 'music/club1.mp3'
	elseif clubrand = 2:
		$tracklist_club[0] = 'music/club2.mp3'
	elseif clubrand = 3:
		$tracklist_club[0] = 'music/club3.mp3'
	elseif clubrand = 4:
		$tracklist_club[0] = 'music/club4.mp3'
	elseif clubrand = 5:
		$tracklist_club[0] = 'music/club5.mp3'
	end
end

