# FSroom
minut+=1
gs'stat'
gs'FSstat'
'<center><B>street</B></center>'



     act 'Go into the room':
     cls
	 FShour+=1
	 minut+=1
	 gs'stat'
	 !'You left the room.'
	 gt 'FSgame'
     end
     
     act 'wander':
     cls
	 FShour+=1
	 minut+=1
	 gs'stat'
	 'You wander through the deserted streets.'
	 act 'further':gt $curloc
     end


