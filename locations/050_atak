if $ARGS[0]='uronKritV':
cla
set damage = RAND(strenV,strenV*2) + weapbonusV + magweapbonusV
     if knifeV > 0:
     set knibonV = knifeV+1
     set knifeV = 0
     set damage = damage*knibonV
     set knibonV = 0
     '<B><font color=red>As you fly throwing knife</font></B>'
     end
     if defence <= 0 and klon <= 0 and unmaterial <= 0 and barier <= 0:
     if dospeh > 0:set damage = damage - dospehDef&set dospehEn = dospehEn - 1
     if damage < 0:set damage = 0
     Set health = health - damage
         if weaponTipeV = 1:set blood = blood + RAND (weaponV/4,weaponV/2)
         if weaponTipeV = 2:set stunV = stunV + RAND (1,2)
         if weaponTipeV = 3:set blood = blood + RAND (weaponV/4,weaponV/2)
         if stunnerV > 0:
         set stunnerV = 0
         set stun = RAND (3,6)
         '<B><font color=red><<$nameV>> paralyze you on <<stun>> moves.</font></B>'
         end
     '<B><font color=red>You harmed <<damage>> units</font></B>'
         if zerkalo > 0:
         set zerkalo = zerkalo - 1
         set healthV = healthV - damage
         '<B><font color=green>You have reflected damage and <<$nameV>> received <<damage>> points of damage</font></B>'
         end
     xgt'fight','sta'
     end
     if defence > 0 and klon <= 0 and unmaterial <= 0:
     Set defence = defence - damage
     '<B><font color=red>Your defense harmed <<damage>> units</font></B>'
     xgt'fight','sta'
     end
     if klon > 0 and unmaterial <= 0:
     set klon = klon - 1
     '<B><font color=red>Destroy your clone, Do you have more <<Klon>> clones</font></B>'
     xgt'fight','sta'
     end
     if unmaterial > 0:
     set unmaterial = unmaterial - 1
     '<B><font color=green>Beat the enemy flew right through your body. YOU are not material</font></B>'
     xgt'fight','sta'
     end
     if barier > 0:
     !imeniteta barrier for damage
         if barier >= damage:
         '<B><font color=green>You can easily withstand the impact of the enemy</font></B>'
         xgt'fight','sta'
         end
         if barier < damage:
         Set health = health - damage
         if weaponTipeV = 1:set blood = blood + RAND (weaponV/4,weaponV/2)
         if weaponTipeV = 2:set stunV = stunV + RAND (1,2)
         if weaponTipeV = 3:set blood = blood + RAND (weaponV/4,weaponV/2)
         if stunnerV > 0:
         set stun = RAND (3,6)
         '<B><font color=red><<$nameV>> paralyze you on <<stun>> moves.</font></B>'
         end
         '<B><font color=red>You harmed <<damage>> units</font></B>'
             if zerkalo > 0:
             set zerkalo = zerkalo - 1
             set healthV = healthV - damage
             '<B><font color=green>You have reflected damage and <<$nameV>> received <<damage>> points of damage</font></B>'
             end
         end
     xgt'fight','sta'
     end
