	end

	if judosparday ! day:
		act 'Sparring (Juugo, mutant power 600)':
		cla
		set judosparday = day
		set $nameV = 'Juugo'
		!Статы
		set strenV = 600
		set speedV = 60
		set agilV = 60
		set reactV = 50
		set vitalV = 200
		set intelV = 50
		set willV = 50
		!Навыки
		set magikV = 100
		set boxingV = 100
		set shootV = 100
		!Параметры
		set healthV = vitalV * 10 + strenV * 5
		set mannaV = intelV * 10 + magikV * 10
		set willpowerV = willV * 10
		set BonusAtakV = 0
		set BonusDefV = 0
		!программа действий
		set program = 0
		set jugoFight = 2
		gt 'fight','start'
		end
	end

	if karinsparday ! day:
		act 'Sparring (Karin mag6)':
		cla
		set karinsparday = day
		set $nameV = 'Karin'
		!Статы
		set strenV = 60
		set speedV = 100
		set agilV = 100
		set reactV = 60
		set vitalV = 100
		set intelV = 100
		set willV = 100
		!Навыки
