act 'Go outside':
	minut += 5 & gt'Gaddvor'
end

act 'Eat':
	cla
	*clr
	gs'stat'

	frost=0
	'<center><img src="images/pics/food.jpg" ></center>'
	set minut += 30
	set health += 10
	set manna += 20
	wipo += 5
	salo += 1

	if energy >= 30:
		set fat += 5
		'You can no longer have.'
	end

	if energy >= 20 and energy < 30:
		set fat += 3
		set energy += 10
		'You pushed through the power of a meal.'
	end

	if energy < 20:
		set energy += 20
		!set fat = fat + 1
		'You gladly ate cooked food.'
	end

	if water >= 20:
		'Tea in you no longer climbs.'
	end

	if water < 20:
		'You gladly drank a cup of tea.'
		set water += 20
	end

	gs 'stat'

	act 'Get up from the table':
		gt $locM, $metkaM
	end
end

