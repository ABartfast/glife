if car = 1:
<<<<<<< 450_avb
	if wrek = 1 or teh <= 0: set bupay = RAND(10000,20000)
	if wrek = 0:
	if teh >= tehcar * 75/100 and wrek = 0: set bupay = RAND(50000,100000)
	if teh < tehcar*75/100 and teh >= tehcar * 50/100 and wrek = 0: set bupay = RAND(40000,60000)
	if teh < tehcar*50/100 and teh >= tehcar * 25/100 and wrek = 0: set bupay = RAND(20000,40000)
	if teh < tehcar*25/100 and wrek = 0: set bupay = RAND(10000,20000)
	end
=======
