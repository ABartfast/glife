     act 'GO_OUT':
     cla
     set minut = minut + 5
     gt'down'
     end
     
     if student = 0 and abiturient = 0 and money >= 15000 and month < 9 and diplom = 0 and age>=17:
     ''
         act 'Pay preparatory courses 15 sput':
		 cla
