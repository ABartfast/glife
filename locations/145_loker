cla
if numdres=131:newcloB[131] = bedra&newcloH[131]=100
'<center><img src="ode/newclo/<<numdres>>.jpg" ></center>'
'(remaining strength <<newcloH[numdres]>>, The size of the hips <<newcloB[numdres]>>)'
act 'In the closet':gt'loker','start'
if newcloH[numdres] > 0 and newcloB[numdres] >= bedra - 8 and newcloB[numdres] <= bedra + 8:
	 act 'Put_On':
		 set numdress=numdres
		 set odetjeans=0
		 set odetyoga=0
		 set odetsarafan=0
		 set odetshort=0
		 set odetskirt=0
		 set odetdress=0
		 set odetprofi=0
		 set odetpants=0
		 set odetlatex=0
		 set odethooker=0
		 set odetkis=0
		 set odetnewclo=1
		 set odetero=0
		 set vatnik=0
		 set glamur = 1
		 set sport = 0
		 set ski = 0
		 Sforma = 0
		 
		 if numdress<90:set ski = 1
		 if numdress=131:set Sforma = 1&glamur=0&ski = 1
		 set hook = 0
		 set ofclo = 0
		 set sexual = 1
		 if cumfrot > 0:set cumfrot = 0
		 set nude = 0
		 set odevnesh = 5
		 gt'loker','start'
		 end
	 end
if newcloH[numdres] <= 0:
	 'This thing is worn and is not suitable for further wear.'
	 set newclo[numdres] = 0
	 set newcloH[numdres] = 0
	 set nude = 1
	 act 'Select another thing':gt'loker','start'
	 end
if newcloB[numdres] < bedra - 8 or newcloB[numdres] > bedra + 8:
	 'This thing is you no longer have the right size and not suitable for further wear.'
	 act 'Select another thing':gt'loker','start'
	 end
end

