if $ARGS[0] = 'ero1':
cla
*clr
'<center><img src="img/dress/ero<<numdress>>.jpg" ></center>'
act 'ago':gt'loker','start'
act 'Put_On':
	 set odetjeans=0
	 set odetyoga=0
	 set odetsarafan=0
	 set odetshort=0
	 set odetskirt=0
	 set odetdress=0
	 set odetprofi=0
	 set odetpants=0
	 set odetlatex=0
	 set odethooker=0
	 set odetkis=0
	 set odetnewclo=0
	 set odetero=1
	 set vatnik=0
	 set glamur = 0
	 set sport = 0
	 set ski = 0
	 set hook = 0
	 set ofclo = 0
	 set sexual = 0
	 if cumfrot > 0:set cumfrot = 0
	 set nude = 0
	 set odevnesh+=10
	 set odetero=numdress
	 gt'loker','start'
	 end
end

if $ARGS[0]='jeans':
cla
'<center><img src="ode/jeans<<numdres>>.jpg" ></center>'
'(remaining strength <<jeansH[numdres]>>, The size of the hips <<jeansB[numdres]>>)'
if mag=1:
	 'price <<payclo>>'
	 act 'remove item.':GOTO $loc, $metka
	 if jeans[numdres]=0 and money>=payclo:
		 act 'buy':
			 money-=payclo
			 jeans[numdres]=1
			 jeansH[numdres]=60
			 jeansB[numdres]=bedra
			 GOTO $loc, $metka
			 end
		 end
	 end
if mag=0:
	 act 'In the closet':gt'loker','start'
	 if jeansH[numdres] > 0 and jeansB[numdres] >= bedra - 8 and jeansB[numdres] <= bedra + 8:
		 act 'Put_On':
			 set numdress=numdres
			 set odetjeans=1
			 set odetyoga=0
			 set odetsarafan=0
			 set odetshort=0
			 set odetskirt=0
			 set odetdress=0
			 set odetprofi=0
			 set odetpants=0
			 set odetlatex=0
			 set odethooker=0
			 set odetkis=0
			 set odetnewclo=0
			 set odetero=0
			 set vatnik=0
			 set glamur = 0
			 set sport = 0
			 set ski = 0
			 set hook = 0
			 set ofclo = 0
			 set sexual = 0
			 if cumfrot > 0:set cumfrot = 0
			 set nude = 0
			 set odevnesh = 1
			 gt'loker','start'
			 end
		 end
	 if jeansH[numdres] <= 0:
		 'This thing is worn and is not suitable for further wear.'
		 set jeans[numdres] = 0
		 set jeansH[numdres] = 0
		 set nude = 1
		 act 'Select another thing':gt'loker','start'
		 end
	 if jeansB[numdres] < bedra - 8 or jeansB[numdres] > bedra + 8:
		 'This thing is you no longer have the right size and not suitable for further wear.'
		 act 'Select another thing':gt'loker','start'
		 end
	 end
end

