	 set numdress=0
	 set odetjeans=0
	 set odetyoga=0
	 set odetsarafan=0
	 set odetshort=0
	 set odetskirt=0
	 set odetdress=0
	 set odetprofi=0
	 set odetpants=0
	 set odetlatex=0
	 set odethooker=0
	 set odetkis=0
	 set odetnewclo=0
	 set odetero=0
	 set glamur = 0
	 set sport = 0
	 set ski = 0
	 set hook = 0
	 set ofclo = 0
