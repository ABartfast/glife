         act 'mopping the floor (1 h 100 RUB)':
         cla
         *clr
         set posudaAday = day
         set minut = minut + 60
         set posuda = posuda + 1
         set manna = manna - 15
         set money = money + 100
         set sweat = sweat + 1
         '<center><img src="pic/clener1.jpg" ></center>'
         'You are within an hour namyvali floor in a diner. After that 100 rubles for a job.'
         act 'Get_Away':gt'burger','start'
         end
     end

end
