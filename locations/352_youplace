# youplace
$metka = $ARGS[0]
$loc = $CURLOC
'<center><H4>Your land</H4></center>'
if placeMansion<2:'<center><img src="pic/yzemelniy.jpg" ></center>'
if placeMansion=2:'<center><img src="pic/ystart.jpg" ></center>'
if placeMansion=3:'<center><img src="pic/yosobnak.jpg" ></center>'
''
clr
gs'stat'
gs'time'

if car > 0 and cardrive = 18:
set cardrive = 18
'In the parking lot is worth <a href="exec:GS ''carF'',''start''">your <<$car>></a>.'
end


act 'Exit the village':minut+=15&gt'locmansion'

if bassMansion=1:'<a href="exec:gt ''ybass''">Swimming_Pool</a>.'
if banaMansion>=1:'<a href="exec:gt ''yban''">bath</a>.'
if placeMansion=3:'<a href="exec:gt ''ymans''">mansion</a>.'

if karta>=1500000 and placeMansion=0:
     act 'Buy land for cottage 1,5 million':
     cls
     karta-=1500000
     placeMansion=1
     minut+=5
     gs'stat'
     'You bought a plot cottage'
     act 'Get_Away':gt $curloc
     end
end


if karta>=500000 and placeMansion=1:
     act 'Pay for supply of communications and building foundations 500 sput':
     cls
     karta-=500000
     placeMansion=2
     minut+=5
     gs'stat'
     'You paid for a supply of communications to the site '
     act 'Get_Away':gt $curloc
     end
end

