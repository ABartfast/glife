if GvstreBoy = 0:
	act 'Meet me in the park':
	cls
	minut += 3
	GvstreBoy = 1
	GvstreBoyAfter[numnpcboy] = 1
	SkverBoy = numnpcboy
	gs 'stat'
	'You are invited to meet in the park.'
	act 'Hangup': gt 'korrPar'
	end
