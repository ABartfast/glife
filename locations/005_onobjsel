$d_cheats_menu_rep = {
	gs 'stat'
	clr

	pl '<a href="exec:dynamic $din_cheat"><B>Cheat Menu</B></a>'
	nl

	if gorslut > 0:
		pl 'In town you are known as <<$gnikname>>'
	end

	if gorslut = 0:
		pl 'You have a good reputation in the town'
	end

	if gorslut ! 1:
		pl '<a href="exec:gorslut = 1 & dynamic $d_cheats_menu_rep">Change reputation to "flirt"</a>'
	end

	if gorslut ! 2:
		pl '<a href="exec:gorslut = 2 & dynamic $d_cheats_menu_rep"> Change reputation to "cocksucker"</a>'
	end

	if gorslut ! 3:
		pl '<a href="exec:gorslut = 3 & dynamic $d_cheats_menu_rep">Change reputation on to "total cocksucker"</a>'
	end

	if gorslut ! 4:
		pl '<a href="exec:gorslut = 4 & dynamic $d_cheats_menu_rep">Change reputation to "easy"</a>'
	end

	if gorslut ! 5:
		pl '<a href="exec: gorslut = 5 & dynamic $d_cheats_menu_rep">Change reputation to "slut"</a>'
	end

	if gorslut ! 6:
		pl '<a href="exec:gorslut = 6 & dynamic $d_cheats_menu_rep">Change reputation to "prostitute"</a>'
	end

	if gorslut > 0:
		pl '<a href="exec:gorslut = 0 & dynamic $d_cheats_menu_rep">Clear reputation in town</a>'
	end

	if opusk > 0 or sipovka > 0 or rozetka > 0:
		pl '<a href="exec:opusk = 0 & sipovka = 0 & rozetka = 0 & dynamic $d_cheats_menu_rep">Clear reputation with Gopnik</a>'
	end
}

