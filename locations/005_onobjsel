	gs 'stat'
	clr

	if cheatHapri = 0:
		$cheatHapri = 'OFF'
	end

	if cheatHapri = 1:
		$cheatHapri = 'ON'
	end

	if cheatNomakeup = 0:
		$cheatNomakeup = 'OFF'
	end

	if cheatNomakeup = 1:
		$cheatNomakeup = 'ON'
	end

	if cheatNosweat = 0:
		$cheatNosweat = 'OFF'
	end

	if cheatNosweat = 1:
		$cheatNosweat = 'ON'
	end

	if cheatNoEat = 0:
		$cheatNoEat = 'OFF'
	end

	if cheatNoEat = 1:
		$cheatNoEat = 'ON'
	end

	pl '<a href="exec:dynamic $dex_cheat">DeX cheat menu</a>'
	pl '<a href="exec:dynamic $din_cheat"><B>Cheat Menu</B></a>'
	nl
	pl 'Option to <a href="exec:dynamic $din_cheat_hapri">is always groomed</a> <<$cheatHapri>>'
	pl 'Option to <a href="exec:dynamic $din_cheat_nomakeup">never smeared cosmetics</a> <<$cheatNomakeup>>'
	pl 'Option to <a href="exec:dynamic $din_cheat_Nosweat">never sweat</a> <<$cheatNosweat>>'
	pl 'Option to <a href="exec:dynamic $din_cheat_NoEat">never to eat or drink</a> <<$cheatNoEat>>'
}

