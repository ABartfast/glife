$d_cheats_menu_vnesh = {
	gs 'stat'
	clr

	pl '<a href="exec:dynamic $din_cheat"><B>Cheat Menu</B></a>'
	nl
	pl '<a href="exec:dynamic $din_cheat_param">See the design parameters of the body.</a>'
	nl
	pl 'Your <<$lip1>> lips.'

	if lip < 4:
		pl 'Option to <a href="exec:lip += 1 & dynamic $d_cheats_menu_vnesh">enlarge the lips</a>'
	end

	if lip > 0:
		pl 'Option to <a href="exec:lip -= 1 & dynamic $d_cheats_menu_vnesh">reduce the lips</a>'
	end

	nl
	pl 'Your <<$skin1>>, <<$tan>> skin.'

	if skin < 4:
		pl 'Option to <a href="exec:skin += 1 & dynamic $d_cheats_menu_vnesh">improve the skin</a>'
	end

	if skin > 0:
		pl 'Option to <a href="exec:skin -= 1 & dynamic $d_cheats_menu_vnesh">impair the skin</a>'
	end

	if tan >= 0:
		pl 'Option to <a href="exec:tan += 10 & dynamic $d_cheats_menu_vnesh">add tan</a>'
	end

	if tan > 0:
		pl 'Option to <a href="exec:tan = 0 & dynamic $d_cheats_menu_vnesh">remove tan</a>'
	end

	nl
	pl '<<$glaza>>'

	if resnic < 2:
		pl 'Option to <a href="exec:resnic += 1 & dynamic $d_cheats_menu_vnesh">increase eyelashes</a>'
	end

	if resnic > 0:
		pl 'Option to <a href="exec:resnic -= 1 & dynamic $d_cheats_menu_vnesh">reduce the eyelashes</a>'
	end

	if glaraz < 3:
		pl 'Option to <a href="exec:glaraz += 1 & dynamic $d_cheats_menu_vnesh">enlarge the eyes</a>'
	end

	if glaraz > 0:
		pl 'Option to <a href="exec:glaraz -= 1 & dynamic $d_cheats_menu_vnesh">shrink the eyes</a>'
	end

	nl

	if salo > 0:
		pl 'Option to <a href="exec:salo -= 5 & fat = salo & dynamic $d_cheats_menu_vnesh">reduce fat</a> <a href="exec:salo = 0 & fat = salo & dynamic $d_cheats_menu_vnesh">Zero fat</a>'
	end

	pl 'Option to increase fat by <a href="exec:salo += 5 & fat = salo & dynamic $d_cheats_menu_vnesh">5</a>, <a href="exec:salo += 10 & fat = salo & dynamic $d_cheats_menu_vnesh">10</a>, <a href="exec:salo += 25 & fat = salo & dynamic $d_cheats_menu_vnesh">25</a>, <a href="exec:salo += 50 & fat = salo & dynamic $d_cheats_menu_vnesh">50</a>, <a href="exec:salo += 100 & fat = salo & dynamic $d_cheats_menu_vnesh">100</a>'
	nl
	pl 'Current age is <<age>>, apparent age is <<vidage>>'

	if age >= 0:
		pl 'Option to <a href="exec:dynamic $vozr1">change the age</a>'
	end

	if vidage >= 0:
		pl 'Option to <a href="exec:dynamic $vozr2">change apparent age</a>'
	end

	pl 'Option to <a href="exec:dynamic $d_cheats_menu_tatoo">remove tattoo</a>'
}

$vozr1 = {
	clr

	age = input ("Enter actual age. Minimum is 15 years?")

	if age <= 15:
		age = 15
	end

	pl '<a href="exec:dynamic $d_cheats_menu_vnesh"><B>Update</B></a>'
}

$vozr2 = {
	clr

	vidage = input ("Enter apparent age. Minimum is 13 years?")
	if vidage <= 13:
		vidage = 13
	end

	pl '<a href="exec:dynamic $d_cheats_menu_vnesh"><B>Update</B></a>'
}

