$din_cheat_hapri = {
	if cheatHapri = 0:
		cheatHapri = 1
		gs 'stat'
		dynamic $din_cheat
		exit
	else
		cheatHapri = 0
		gs 'stat'
		dynamic $din_cheat
	end
}

$din_cheat_nomakeup = {
	if cheatNomakeup = 0:
		cheatNomakeup = 1
		gs 'stat'
		dynamic $din_cheat
		exit
	else
		cheatNomakeup = 0
		gs 'stat'
		dynamic $din_cheat
	end
}

$din_cheat_Nosweat = {
	if cheatNosweat = 0:
		cheatNosweat = 1
		gs 'stat'
		dynamic $din_cheat
		exit
	else
		cheatNosweat = 0
		gs 'stat'
		dynamic $din_cheat
	end
}

$din_cheat_NoEat = {
	if cheatNoEat = 0:
		cheatNoEat = 1
		gs 'stat'
		dynamic $din_cheat
		exit
	else
		cheatNoEat = 0
		gs 'stat'
		dynamic $din_cheat
	end
}

$din_cheat_param = {
	gs 'stat'
	clr
	pl '<a href="exec:dynamic $din_cheat">Back to the cheat menu</a>'
	pl '<<strenbuf>> - strenbuf'
	pl '<<vitalbuf>> - vitalbuf'
	pl '<<vmeat>> - vmeat'
	pl '<<vfat>> - vfat'
	pl '<<Kves>> - Kves'
	pl '<<krost>> - krost'
	pl '<<ves>> - ves'
	pl '<<talia>> - talia - Waist measurement in cm'
	pl '<<grudi>> - grudi - Chest measurement in cm'
	pl '<<bedra>> - bedra'
	pl '<<titK>> - tits'
	pl '<<silicone>> - silicone'
	pl '<<grudi>> - <<talia>> - <<bedra>>, breast size <<tits>>, the difference between the hips and waist <<bedra-talia>> cm'
	pl 'Indicator muscularity <<musle>> - musle, <<salo>> - salo <<salo * 2>> - salo * 2 <<fat>> - fat'
}

$din_cheat = {
	gs 'stat'
	clr
	if cheatHapri = 0:$cheatHapri = 'OFF'
	if cheatHapri = 1:$cheatHapri = 'ON'
	if cheatNomakeup = 0:$cheatNomakeup = 'OFF'
	if cheatNomakeup = 1:$cheatNomakeup = 'ON'
	if cheatNosweat = 0:$cheatNosweat = 'OFF'
	if cheatNosweat = 1:$cheatNosweat = 'ON'
	if cheatNoEat = 0:$cheatNoEat = 'OFF'
	if cheatNoEat = 1:$cheatNoEat = 'ON'

	pl 'If you use the cheats, then remember, It`s your decision and the consequences respond to you.'
	pl '<a href="exec:dynamic $din_cheat_param">Show design parameters of the body.</a>'
	pl '<a href="exec:health = vital * 10 + stren * 5 + 1000 & willpower = intel * 5 + will * 5 + 1000 & manna = (intel * magik) + magik * 100 + vital * 10 + rikudo & dynamic $din_cheat">Complete recovery, manna, reason.</a>'

	if salo ! fat:pl 'option <a href="exec:fat = salo & dynamic $din_cheat">Balance the fat</a>'

	pl '<b>Permanent Cheats</b>'
	pl 'Option to <a href="exec:dynamic $din_cheat_hapri">is always groomed</a> <<$cheatHapri>>'
	pl 'Option to <a href="exec:dynamic $din_cheat_nomakeup">never smeared cosmetics</a> <<$cheatNomakeup>>'
	pl 'Option to <a href="exec:dynamic $din_cheat_Nosweat">never sweat</a> <<$cheatNosweat>>'
	pl 'Option to <a href="exec:dynamic $din_cheat_NoEat">never to eat or drink</a> <<$cheatNoEat>>'
	pl '<b>Cheats designer body</b>'

	if lip < 4:pl 'Option to <a href="exec:lip += 1 & dynamic $din_cheat">enlarge the lips</a>'
	if lip > 0:pl 'Option to <a href="exec:lip -= 1 & dynamic $din_cheat">reduce the lips</a>'
	if skin < 4:pl 'Option to <a href="exec:skin += 1 & dynamic $din_cheat">improve the skin</a>'
	if skin > 0:pl 'Option to <a href="exec:skin -= 1 & dynamic $din_cheat">impair the skin</a>'
	if resnic < 2:pl 'Option to <a href="exec:resnic += 1 & dynamic $d_cheat">increase eyelashes</a>'
	if resnic > 0:pl 'Option to <a href="exec:resnic -= 1 & dynamic $d_cheat">reduce the eyelashes</a>'
	if glaraz < 3:pl 'Option to <a href="exec:glaraz += 1 & dynamic $d_cheat">enlarge the eyes</a>'
	if glaraz > 0:pl 'Option to <a href="exec:glaraz -= 1 & dynamic $din_cheat">shrink the eyes</a>'
	if salo > 0:pl 'Option to <a href="exec:salo -= 5 & fat = salo & dynamic $d_cheat">reduce fat</a> <a href="exec:salo = 0 & fat = salo & dynamic $din_cheat">Zero fat</a>'
	pl 'Option to increase fat by <a href="exec:salo += 5 & fat = salo & dynamic $d_cheat">5</a>, <a href="exec:salo += 25 & fat = salo & dynamic $din_cheat">25</a>, <a href="exec:salo += 50 & fat = salo & dynamic $din_cheat">50</a>, <a href="exec:salo += 100 & fat = salo & dynamic $din_cheat">100</a>'

	pl 'Increase power by <a href="exec:stren += 1 & dynamic $din_cheat">1</a> <a href="exec:stren += 5 & dynamic $din_cheat">5</a> <a href="exec:stren += 10 & dynamic $din_cheat">10</a> <a href="exec:stren = 100 & dynamic $din_cheat">set to 100</a>'
	if stren > 0:pl 'Decrease power by <a href="exec:stren -= 1 & dynamic $d_cheat">1</a> <a href="exec:stren -= 5 & dynamic $din_cheat">5</a> <a href="exec:stren -= 10 & dynamic $din_cheat">10</a>'

