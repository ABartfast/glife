	!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
	pl 'Attractiveness <<vnesh>>'
	pl 'Power <<stren>>'
	pl 'Speed ​​<<speed>>'
	pl 'Agility <<agil>>'
	pl 'Endurance <<vital>>'
	pl 'Intelligence <<intel>>'
	pl 'Willpower <<will>>'
	pl 'Reaction <<react>>'
	pl '<<$fat>>'
	!!pl 'Bezstydstvo <<shameless>>'

	if SUB > 0:pl 'Subordination <<SUB>>'
	if DOM > 0:pl 'Dominance <<DOM>>'

	!!pl 'Muscles <<$musle>>'

	if magik > 0:pl 'Magic <<magik>>'
	if Jab > 0:pl 'Jab - <<Jab>>'
