$buyfood={
if hour >= 8 and hour <= 20:
     if money >= 50:
	 cla
	 *clr
	 set minut = minut + 15
	 portion = input ("How many servings you want to buy?")
		 if portion<=0:portion=1
		 if portion*50 > money:
		 'You do not have enough money.'
		 end
		 if portion*50 <= money:
		 set eda = eda + portion
		 set money = money - portion*50
		 '<center><img src="pic/kassa.jpg" ></center>'
		 'You bought food and paid the money to the cashier.'
		 end
	 act 'Move away from the counter':gt'shop','food'
	 else
	 'You do not have enough money'
	 end
else
'The department is closed'
end
}
