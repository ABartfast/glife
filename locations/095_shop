'The department is closed'
end
}
$buycd={
if hour >= 8 and hour <= 20:
     if money >= 500 and komp>0 and GameCD=0:
	 cla
	 *clr
	 set money = money - 500
	 GameCD = 1
	 '<center><img src="pic/kassa.jpg" ></center>'
	 'You get paid in cash and bought the game'
	 act 'Get_Away':gt'shop','teh'
	 elseif komp=0:
	 'You do not have a computer'
	 elseif GameCD>0:
	 'You already have this game'
	 else
	 'You do not have enough money'
	 end
else
'The department is closed'
end
}
