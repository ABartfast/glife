$buytv={
if hour >= 8 and hour <= 20:
     if money >= 5000 and TV < 1:
	 cla
	 *clr
	 set money = money - 5000
	 set TV = 1
	 '<center><img src="pic/kassa.jpg" ></center>'
	 'You get paid in cash and bought a TV'
	 act 'Get_Away':gt'shop','teh'
	 elseif TV >0:
	 'Do you already have a TV'
	 else
	 'You do not have enough money'
	 end
else
'The department is closed'
end
}
$buytv2={
if hour >= 8 and hour <= 20:
     if money >= 35000 and TV < 2:
	 cla
	 *clr
	 set money = money - 35000
	 set TV = 2
