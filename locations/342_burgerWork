$dinBistroWork = {
	act 'Dinner':
	cls
	gs 'stat'
	'<center><img src="images\pics\food.jpg"></center>'
	set minut += 30
	set health += 10
	set manna += 5
	set willpower += 5
	'You took bystroezhke food and sat at a table for dinner.'
		if energy >= 30:
		set fat += 8
		'You can no longer have.'
		end
		if energy >= 20 and energy < 30:
		set fat += 6
		set energy += 10
		'You pushed through the power of a meal.'
		end
		if energy < 20:
		set energy += 20
		set fat += 4
		'You gladly ate the food.'
		end
		if water >= 20:
		'Tea in you no longer climbs.'
		end
		if water < 20:
		'You gladly drank a mug of tea.'
		set water += 20
		end
	gs 'stat'

		dynamic $dinBistroWork3
	end
