# preg
if mesec = 0 and preg = 0 and tabletki = 0:
!no monthly
set pregrand = rand(0, 100)
	if pregrand >= 93:
		$father = $boy
		set preg = 1
	end
	if tabletki > 0:
		set pregrand = rand(0, 200)

		if pregrand = 0:
			$father = $boy
			set preg = 1
		end
	end
end

