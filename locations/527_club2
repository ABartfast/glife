	if $ARGS[1]<>'' or ($ARGS[0]<>'' and LOC $ARGS[0]):
		<<$otg_Menu_Current>>_time=MSECSCOUNT-<<$otg_Menu_Current>>_time
		otg_Menu_Time=<<$otg_Menu_Current>>_time
		<<$otg_Menu_Current>>_selected=-1
