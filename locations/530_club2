			dynamic $ARGS[1]"+dyneval("$Result=$<<$otg_Menu_Current>>_parameters['<<ARGS[0]>>_P']")+"
		else
			GS $ARGS[0]"+dyneval("$Result=$<<$otg_Menu_Current>>_parameters['<<ARGS[0]>>_P']")+"
		end
	else
		MSG 'location ''<<$ARGS[0]>>'' found!'
	end
",$ARGS[0],$ARGS[1]

