$budgetpants = {
	cla

	act 'Remove item.':gt $loc, $metka

	if pants[i] = 0 and money >= 1750:
		act 'Buy (1,750 rubles)':
			money -= 1750
			pants[i] = 1
			pantsH[i] = 60
			pantsB[i] = bedra
			gt $loc, $metka
		end
	else
