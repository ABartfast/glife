	if karta >= 30000 and dachaest = 0:
		act 'Buy a cottage for 30,000 rubles':
		cls
		karta -= 30000
		dachaest = 1
		minut += 15
		etoexhib = 0
		gs 'stat'
		'You bought a house with a garden in the dacha cooperative at 5 km. along the northern road.'
		act 'Leave': gt $curloc
		end
	end

	if karta >= 1500000 and placeMansion = 0:
