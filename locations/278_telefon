$telotkaz={
cla
set minut+=10
'<<$telsob>>: I`m sorry I`m busy, Come next time.'
act 'hang Up': gt'telefon','fin'
}

$telotkazboy={
cla
set minut+=10
'<<$boyA>>: Sorry , Come next time.'
act 'hang Up': gt'telefon','fin'
}

if $ARGS[0]='start':
cla
clr
gs'stat'

if vladimirday!day and vladimirQW=20 and hour = 16:gt 'qwloc2'
if vladimirday!day and vladimirQW=25 and hour = 16 and week=6:gt 'qwloc3'
if vladimirday!day and vladimirQW=35 and hour = 16 and week=6:GT 'qwloc4'
if QWdogreiqTimer=0 and QWdogReiq=2 and week=7 and qwdogiventday!day and hour>=12:GT 'qwlocdog'
	 if ring = 0:
	 if number = 6:'<center><B><<num6>><<num5>>-<<num4>><<num3>>-<<num2>><<num1>></B></center>'
	 if number = 5:'<center><B><<num6>><<num5>>-<<num4>><<num3>>-<<num2>></B><<num1>></center>'
	 if number = 4:'<center><B><<num6>><<num5>>-<<num4>><<num3>></B>-<<num2>><<num1>></center>'
	 if number = 3:'<center><B><<num6>><<num5>>-<<num4>></B><<num3>>-<<num2>><<num1>></center>'
	 if number = 2:'<center><B><<num6>><<num5>></B>-<<num4>><<num3>>-<<num2>><<num1>></center>'
	 if number = 1:'<center><B><<num6>></B><<num5>>-<<num4>><<num3>>-<<num2>><<num1>></center>'
	 if number = 0:'<center><<num6>><<num5>>-<<num4>><<num3>>-<<num2>><<num1>></center>'
	 *nl
	 '<center><B><a href="exec:GT ''telefon'',''1''">1</a>   <a href="exec:GT ''telefon'',''2''">2</a>   <a href="exec:GT ''telefon'',''3''">3</a></B></center>'
	 '<center><B><a href="exec:GT ''telefon'',''4''">4</a>   <a href="exec:GT ''telefon'',''5''">5</a>   <a href="exec:GT ''telefon'',''6''">6</a></B></center>'
	 '<center><B><a href="exec:GT ''telefon'',''7''">7</a>   <a href="exec:GT ''telefon'',''8''">8</a>   <a href="exec:GT ''telefon'',''9''">9</a></B></center>'
	 '<center><B><a href="exec:GT ''telefon'',''reset''">reset</a>   <a href="exec:GT ''telefon'',''0''">0</a>   <a href="exec:GT ''telefon'',''challenge''">challenge</a></B></center>'
     
     !etogame
if hour>=8 and hour <=19:
     if telalla>0:
		 act 'call Allais':
			 $telsob='Alla'
			 dynamic $objmobilaraz
			 end
		 end
	 if telmasha>0:
		 act 'call Masha':
			 $telsob='Masha'
			 dynamic $objmobilaraz
			 end
		 end
	 if telkat>0:
		 act 'call Kate':
			 $telsob='katya'
			 dynamic $objmobilaraz
			 end
		 end
	 if bfA>0 and svidanieA=0:
		 act 'call <<$boyA>>':dynamic $objmobilarazboy
		 end
	 end
		if nastjaqw=1 and week=6 and vnesh>20:act 'call Nastya':gt 'nastja','pos4'
		if nastjaqw=2 and week!6 and zenit=0 and nastjaday!day:act 'Chat with Nastya':gt 'nastja', 'pos13' 
		if hour>=8 and hour <=20 and nastjaqw=2 and week=6 and vnesh>20 and nastjaday!day:act 'call Nastya':gt 'nastja','pos12'

		 act 'Hang up':
		 cla
		 gt'telefon','fin'
		 end
	 end
	 
	 if ring = 1:
	     if ringA = 1:
	     gt'telefon','boyA'
	     exit
	     end 
	     
	     if ringB = 1:
	     gt'telefon','boyB'
	     exit
	     end 
	     
	     if ringC = 1:
	     gt'telefon','boyC'
	     exit
	     end 
	 end
	
	
end

if $ARGS[0]='boyA':
cla
clr
gs'stat'
set ring = 0
set ringA = 0
set boydayA = day
set svetrand = RAND (0,5)
if svetrand = 0:set $sveta = 'lapulya'
if svetrand = 1:set $sveta = 'bunny'
if svetrand = 2:set $sveta = 'pussy'
if svetrand = 3:set $sveta = 'small_fish'
if svetrand = 4:set $sveta = 'Svetsik'
if svetrand = 5:set $sveta = 'Sveta'
'hello <<$sveta>> these_are <<$boyA>> how`s it going?'
act 'normally':
	 cla
	 '(Do_You) - normally, as you like?'
	 '(<<$boyA>>) - just great, Here are just miss you, can meet today?'
	 if pregtime<=dayA and pregtime>=7 and pregtalk=0:
		 act 'My_Dear, you`ll be a dad.':
			 cla
			 pregtalk=1
			 '(Do_You) - My_Dear, you`ll be a dad.'
			 if harakBoyA = 0:
				 if bfA>=90:
					 '(<<$boyA>>) - It`s wonderful! We will soon be adding.'
					 set love = 1
					 'You started talking about cohabitation. what <<$boyA>> told you, In principle it, do not mind, but first he wants to introduce you to my parents.'
					 'He warns you, Mom is very demanding and strict, <<$boyA>> speaks, that he is very fond of his mother and does not want to upset her. So you have to look chic, only dress boutique, groomed and curled hair, cosmetics in moderation.'
					 'At the next meetinge, as soon as you `re ready to fully <<$boyA>> will take you to your parents.'
					 act 'Hang up.':gt'telefon','fin'
					 act 'Meet tonight':
						 cla
						 set svidanieA = 1
						 '(Do_You) - You can meet in the evening.'
						 '(<<$boyA>>) - How much to come to your house?'
						 act 'call time.':
							 cla
							 meetday=daystart
							 meethour = input ("What are the hours in which the guy to come. At_The_Moment <<hour>> h, You can only assign a greater number of")
							if meethour<=hour or meethour>23:meethour=20
							 '(Do_You) - Come on in 20 hours.'
							 '(<<$boyA>>) - Fair_Enough, <<$sveta>> then to <<meethour>> hours.'
							 act 'Hang up.':gt'telefon','fin'
							 end
						 end
					 end
				 if bfA<90:
					 '(<<$boyA>>) - I`m not yet ready to become a dad. I`ll give you money for an abortion. Go and get rid of the child.'
					 money+=20000
					 act 'Hang up.':gt'telefon','fin'
					 end
				 end 
			 if harakBoyA = 1:
				 '(<<$boyA>>) - I`m not yet ready to become a dad. I`ll give you money for an abortion. Go and get rid of the child.'
				 money+=20000
				 act 'Hang up.':gt'telefon','fin'
				 end
			 if harakBoyA = 2:
				 '(<<$boyA>>) - what? Yes you ohuel! Somewhere sprained pussy, and now for me to dump. You go on a dick.'
				 set bfA = 0
				 pregtalk=0
				 act 'Hang up.':gt'telefon','fin'
				 end
			 end
		 end
	 if pregtime>dayA and pregtime>=7 and pregtalk=0:
		 act 'My_Dear, you`ll be a dad.':
			 cla
			 '(Do_You) - My_Dear, you`ll be a dad.'
			 '(<<$boyA>>) - How the fuck dad? We meet with you less, What are you pregnant. Sprained whore someone, here and go to him. all. I can no longer call. adieu.'
			 set bfA = 0
			 act 'Hang up.':gt'telefon','fin'
			 end
		 end
	 if week > 1 and week < 5 and workKafe = 1:
		 act 'oh, I`m_sorry, I work today.':
			 cla
			 '(Do_You) - oh, I`m_sorry, I work today.'
			 '(<<$boyA>>) - okay, call tomorrow.'
			 act 'Hang up.':gt'telefon','fin'
			 end
		 end
	 act 'tonight':
		 cla
		 set bfA = bfA + 1
		 set svidanieA = 1
		 meetday=daystart
		 '(Do_You) - You can meet in the evening.'
		 '(<<$boyA>>) - How much to come to your house?'
		 act 'call time.':
			 cla
			 meethour = input ("What are the hours in which the guy to come. At_The_Moment <<hour>> h, You can only assign a greater number of single-digit")
			 if meethour<=hour or meethour>23:meethour=20
			 '(Do_You) - Come on in <<meethour>> hours.'
			 '(<<$boyA>>) - Fair_Enough, <<$sveta>> then to <<meethour>> hours.'
			 act 'Hang up.':gt'telefon','fin'
			 end
		 end
	 act 'I`m busy today. Call tomorrow.':
		 cla
		 set bfA = bfA - 1
		 '(Do_You) - I`m busy today. Call tomorrow.'
		 if bfa <= 0:set bfa = 0&'(<<$boyA>>) - whore, you have my Jam, Come on dick.'
		 if bfa > 0:'(<<$boyA>>) - okay, until then.'
		 act 'Hang up.':gt'telefon','fin'
		 end
	 end
act 'leave':
	 cla
	 set bfA = 0
	 gt'telefon','fin'
	 end
end

