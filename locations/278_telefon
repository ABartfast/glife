# telefon
$objmobilaraz={
cla
set telotvet=RAND(0,10)
'"Yes_I_Am, listen" came a voice from the speaker.'
act 'Invite to movie':
	 cla
	 'DO_YOU: Come with me to the cinema?'
	 if telotvet>3:
	 '<<$telsob>>: Well come on after an hour in the cinema.'
	 act 'go':gt 'kinosvid'
	 end
	 if telotvet<=3:dynamic $telotkaz
	 end
if month>5 and temper>20 and osadki<painweather:
act 'Invite a walk in the park':
	 cla
	 'DO_YOU: Come take a walk in the park.'
	 if telotvet>3:
	 '<<$telsob>>: Well come on after an hour in the park.'
	 act 'go':gt 'parksvid'
	 end
	 if telotvet<=3:dynamic $telotkaz
	 end
	 end
act 'Invite pool':
	 cla
	 'DO_YOU: Come pool.'
	 if telotvet>3:
	 '<<$telsob>>: Well come on an hour in billiards.'
	 act 'go':gt 'billsvid'
	 end
	 if telotvet<=3:dynamic $telotkaz
	 end
act 'Invite a cafe':
	 cla
	 'DO_YOU: Let`s go to the cafe.'
	 if telotvet>3:
	 '<<$telsob>>: Well come on an hour in a cafe.'
	 act 'go':gt 'kafesvid'
	 end
	 if telotvet<=3:dynamic $telotkaz
	 end
act 'Invite casino':
	 cla
	 'DO_YOU: Let`s go to the casino `s go.'
	 if telotvet>3:
	 '<<$telsob>>: Well come on in an hour at the casino.'
	 act 'go':gt 'kazinosvid'
	 end
	 if telotvet<=3:dynamic $telotkaz
	 end
act 'hang Up': gt'telefon','fin'
}

