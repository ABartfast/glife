$dex_debug_loc = {
clr
i = 0
:showdebuglocinfo
if $DBPrevLoc[i] ! '':pl '<<$DBPrevLoc[i]>>'
i += 1
if i < 20:jump 'showdebuglocinfo'
pl '<<$DBCurLoc>>'
}
