pl
pl '<a href="exec:dynamic $dex_cheat_loc_photostudio_NewPhotoDay">new job always available</a> <<$cheatNewPhotoDay>>'
}
$dex_cheat_loc_park = {
gs 'stat'
clr
if pusher = 0:$cheatpusher = 'NO'
if pusher = 1:$cheatpusher = 'YES'
pl '<a href="exec:dynamic $dex_cheat_m_loc">Back</a>'
pl
pl 'Met drug dealer <a href="exec:pusher = 1 & dynamic $dex_cheat_loc_park"><<$cheatpusher>></a>'
}
$dex_cheat_loc_photostudio_NewPhotoDay = {
if cheatNewPhotoDay = 0:
	cheatNewPhotoDay = 1
	gs 'stat'
	dynamic $dex_cheat_loc_photostudio
	exit
end
if cheatNewPhotoDay = 1:
	cheatNewPhotoDay = 0
	gs 'stat'
	dynamic $dex_cheat_loc_photostudio
end
}
