}
$dex_cheat_loc_photostudio = {
gs 'stat'
clr
if cheatNewPhotoDay = 0:$cheatNewPhotoDay = 'OFF'
if cheatNewPhotoDay = 1:$cheatNewPhotoDay = 'ON'
