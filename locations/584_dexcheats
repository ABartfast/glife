!!$dex_add_kid = {
set numkid += 1
set daykid[numkid] = day
set monthkid[numkid] = month
set yearkid[numkid] = year
set haprikid[numkid] = rand(0, 3)
set glacol[numkid] = rand(0, 3)
set polkid[numkid] = rand(0, 1)
set kid += 1
set $kidname[numkid] = 'Test Child<<numkid>>'
}
