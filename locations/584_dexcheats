$dex_cheat_Tailor = {
set o = 0
:jensmet1
	set o += 1
	if jeans[o] = 1 and jeansB[o]!bedra:set jeansB[o] = bedra
	if o ! 5:jump 'jensmet1'
	set o = 0
:shortmet1
	set o += 1
	if short[o] = 1 and shortB[o]!bedra:set shortB[o] = bedra
	if o ! 5:jump 'shortmet1'
	set o = 0
:skirtmet1
	set o += 1
	if skirt[o] = 1 and skirtB[o]!bedra:set skirtB[o] = bedra
	if o ! 5:jump 'skirtmet1'
	set o = 0
:dressmet1
	set o += 1
	if dress[o] = 1 and dressB[o]!bedra:set dressB[o] = bedra
	if o ! 24:jump 'dressmet1'
	set o = 0
:profimet1
	set o += 1
	if profi[o] = 1 and profiB[o]!bedra:set profiB[o] = bedra
	if o ! 2:jump 'profimet1'
	set o = 0
:pantsmet1
	set o += 1
	if pants[o] = 1 and pantsB[o]!bedra:set pantsB[o] = bedra
	if o ! 5:jump 'pantsmet1'
	set o = 0
:latexmet1
	set o += 1
	if latex[o] = 1 and latexB[o]!bedra:set latexB[o] = bedra
	if o ! 2:jump 'latexmet1'
	set o = 0
:hookermet1
	set o += 1
	if hooker[o] = 1 and hookerB[o]!bedra:set hookerB[o] = bedra
	if o ! 5:jump 'hookermet1'
	set o = 0
:odekismet1
	set o += 1
	if odekis[o] = 1 and odekisB[o]!bedra:set odekisB[o] = bedra
	if o ! 10:jump 'odekismet1'
	set o = 0
:newclomet1
	set o += 1
	if newclo[o] = 1 and newcloB[o]!bedra:set newcloB[o] = bedra
	if o ! 130:jump 'newclomet1'
}

$dex_cheat_character_stats = {
gs 'stat'
clr
pl '<a href="exec:dynamic $dex_cheat">Back to DeX cheat menu</a>
<<hairlength>> - Hairlength in mm
<<StoryLine>> - StoryLine
<<MagikDostup>> - MagikDostup
<<umbrella>> - umbrella - Do you own an Umbrella
<<birthday>> - birthday
<<birthmonth>> - birthmonth
<<age>> - Your actual Age
<<vidage>> - How you old you look in years
<<vidageday>> - vidageday - How old you look in days, influenced by makeup and sex.
<<year>> - year - Current Year
<<month>> - month - Current Month
<<week>> - week - Current Week
<<hour>> - hour - Current Hour
<<day>> - day - Current Day
<<birthyear>> - birthyear
<<vmeat>> - vmeat =(strenbuf + vitalbuf) / 8 or (stren + vital) / 8
<<rost>> - Bodyheight in cm
<<fat>> - fat - similar to the stat "salo", but fat only affects you and your appearence, if you use Karen''s spell. But it still keep track of everything you do.
<<salo>> - salo - similar to the stat "fat", but salo affects the appearence, if you <b>DID NOT</b> use the spell at Karen''s
<<musle>> - musle
<<dick>> - dick - Length of the dick of your current sex-partner 
<<energy>> - energy - Your level of hunger - Higher = better
<<water>> - water - Your level thirst - Higher = better
<<son>> - son - How well you are rested - Higher = better
<<stren>> - stren = strenbuf
<<strenbuf>> - strenbuf = stren, but used in stat calculation
<<speed>> - speed - Your level Speed
<<agil>> - agil = agilbuf
<<agilbuf>> - agilbuf = agil, but used in stat calculation
<<vital>> - vital = vitalbuf 
<<vitalbuf>> - vitalbuf = vital, but used in stat calculation
<<intel>> - intel - Your level of Intelligence
<<will>> - will - Your current Will level - DO NOT confuse with will power
<<react>> - react - Your level of Reaction
<<magik>> - magik - You current Magic level
<<health>> - health - Your current health level
<<willpower>> - willpower - Your current amount of willpower - Influenced by Intellect and Will
<<manna>> - manna - You current amount of Mana - Influenced by Intellect and Magic
<<nastjaqw>> - nastjaqw - Nastja Quest - Indicates your friendship
<<money>> - money - Money on your pocket
<<karta>> - karta - Money on your Bankaccount
<<housrA>> - housrA - Checks if you are at home
<<odevnesh>> - odevnesh - Depends on your clothing-type, rates your looks, influences vnesh - Higher = better
<<vnesh>> - vnesh - Your overall looks - Higher = better
<<lobok>> - lobok - Length of the hair in your private area
<<leghair>> - leghair - Length of the hairs on your legs
<<mop>> - mop - Your Make up level - 0 = smeared - 1 = no makeup - 2 = light - 2 = everyday/medium - 3 = heavy
<<housr>> - housr - Do you have your own appartement?
<<tanga>> - tanga - Are you wearing underwear today?
'

}

$cheat_no_leg_hair = {
if cheatNoLegHair = 0:
	cheatNoLegHair = 1
	gs 'stat'
	dynamic $dex_cheat
	exit
end
if cheatNoLegHair = 1:
	cheatNoLegHair = 0
	gs 'stat'
	dynamic $dex_cheat
end
}

