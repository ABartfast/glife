	if alko < 3:pl '<b><font color="red">You´re a little dizzy.</font></b>'
	if alko >= 3 and alko < 6:pl '<b><font color="red">You´re drunk.</font></b>'
	if alko >= 6:pl '<b><font color="red">You´re wasted.</font></b>'
end
