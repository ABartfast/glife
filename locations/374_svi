# svi
if $ARGS[0]='poor':
set minut = minut + 20
     if Venera >= 3:
     set bfA = 0
     '<<$boyA>> you saw signs of venereal disease and left you.'
     act 'Get_Away':gt'street'
     exit
     end
     if sweat > 1:
     !sweating
     set bfA = bfA - 1
         if harakBoyA = 0:'<<$boyA>> tells you, that he appeared urgent business.'
         if harakBoyA = 1:'<<$boyA>> wrinkles her nose in disgust and tells you, that he appeared urgent business.'
         if harakBoyA = 2:'<<$boyA>> tells you, you would do well to wash.'
         if harakBoyA = 3:'<<$boyA>> tells you, that it appeared urgent business.'
         if harakBoyA = 4:'<<$boyA>> wrinkles her nose in disgust and tells you, that it appeared urgent business.'
         if harakBoyA = 5:'<<$boyA>> tells you, you would do well to wash.'        
     act 'Get_Away':gt'street'
     exit
     end
     
     if vatnik = 2:
     !quilted_jacket
     set bfA = bfA - 1
         if harakBoyA = 0:'<<$boyA>> tells you, that he appeared urgent business.'
         if harakBoyA = 1:'<<$boyA>> scornfully looks at you and tells you, that he appeared urgent business.'
         if harakBoyA = 2:'<<$boyA>> tells you, that in this rags you are like a scarecrow.'
         if harakBoyA = 3:'<<$boyA>> tells you, that it appeared urgent business.'
         if harakBoyA = 4:'<<$boyA>> scornfully looks at you and tells you, that it appeared urgent business.'
         if harakBoyA = 5:'<<$boyA>> tells you, that in this rags you are like a scarecrow.'
     act 'Get_Away':gt'street'
     exit
     end
     
     if harakBoyA>0 and izvrat=0 and (cumface > 0 or cumlip > 0):
     set bfA = 0
     '<<$boyA>> I saw you on the traces of semen. He called you slut have cut ties with you.'
     act 'Get_Away':gt'street'
     exit
     end
     
     if daya < 10 and boyonceA = 0:
     !candy buketny period
     set vstrand = RAND (0,2)
         if vstrand = 0:
		 '<<$boyA>> offers a walk in the park.'
			 act 'Walk in the park':
			 cla
			 set minut = minut + 60
			 set fat = fat + 1
			 set bfA = bfA + 1
			 'You walk in the park with a guy. <<$boyA>> telling stories and jokes hunted.'
				 act 'To invite':
				 cla
				 if dayA < 3:set bfA = bfA - 1
                 gt'svidan','kvartA'
                 end
				 
				 act 'Say goodbye and go home':
				 cla
				 set bfA = bfA + 1
				 gt'street'
				 end
			 end
			  
			 act 'Opt_Out':
			 cla
			 set bfA = bfA - 1
			 gt'street'
			 end
         end
         
         if vstrand = 1:
		 '<<$boyA>> offers to go to the cinema.'
			 act 'Go to the cinema':
			 cla
			 set minut = minut + 60
			 set fat = fat + 1
			 set bfA = bfA + 1
			 'You went to the cinema to a new film. <<$boyA>> hug you.'
			     act 'Hug his hand':
			     cla
			     'You watch a movie hugging Man `s hand. Finally.'
			     set bfA = bfA + 1
					 act 'To invite':
					 cla
					 if dayA < 3:set bfA = bfA - 1
					 gt'svidan','kvartA'
					 end
					 
					 act 'Say goodbye and go home':
					 cla
					 set bfA = bfA + 1
					 gt'street'
					 end
                 end
			 end
			  
			 act 'Opt_Out':
			 cla
			 set bfA = bfA - 1
			 gt'street'
			 end
         end
         
         if vstrand = 2:
		 '<<$boyA>> offers pivasika drink in the stairwell.'
			 act 'drink beer':
			 cla
			 set minut = minut + 60
			 set fat = fat + 1
			 set bfA = bfA + 1
			 '<<$boyA>> pivasika bought and crackers and you went with him into the entrance, where drinking and fun chatting.'
				 act 'To invite':
				 cla
				 if dayA < 3:set bfA = bfA - 1
				 gt'svidan','kvartA'
				 end
					
				 act 'Say goodbye and go home':
				 cla
				 set bfA = bfA + 1
				 gt'street'
				 end
			 end
			  
			 act 'Opt_Out':
			 cla
			 set bfA = bfA - 1
			 gt'street'
			 end
         end
     end
     
     if daya >= 10 and boyonceA = 0:
     '<<$boyA>> speaks, that he was a grown man .'
     	 act 'leave':
	     cla
	     set bfA = 0
	     gt'street'
	     end
	     
	     act 'Agreed to have sex':
	     cla
	     *clr
	     set minut = minut + 20
	     'You agreed to have sex. <<$boyA>> delighted.'
	     	 act 'Go to your apartment':gt'svidan','kvartA'
	     	 act 'Go into the bushes':gt'svidan','kustA'
	     end
     exit
     end
     
     if dayA > 15:gs'boylove','haer'
     if dayA > 30:gs'boylove','figure'
     if dayA > 60:gs'boylove','tits'
     
     if boyonceA > 0:
     '<<$boyA>> offers to hang out in the stairwell with pivasikom.'
	     act 'hang_out':
	     cla
	     set minut = minut + 60
	     set fat = fat + 1
	     set bfA = bfA + 1
	     'You drank beer in the hallway with her boyfriend. <<$boyA>> hugged you and said, he wants you.'
	         act 'Go to your apartment':gt'svidan','kvartA'
	         act 'Go into the bushes':gt'svidan','kustA'
	     end
          
         act 'Opt_Out':
         cla
         set bfA = bfA - 1
         gt'street'
         end
     end
end

