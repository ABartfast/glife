	 act 'further':gt $curloc
     end
     
     act 'Walk through the woods to the highway':
     cls
	 KGZhour+=1
	 minut+=1
	 gs'stat'
	 KGZbatl=0
	 KGZplace=201
	 'You went cautiously, trying not to run into dead.'
	 act 'further':gt $curloc
     end

}
