if vitamin > 0:
'You have <<vitamin>> vitaminok.'
	if vitaminday ! day:
		act 'Drink Vitamins':
			cla
			*clr
			set minut += 5
			vitaminday = day
			vitamin -= 1
			frost = 0
			set skinvan += 1
			if skinvan = 20: set skinvan = 0 & set skin += 1
			'You drank Vitaminki'
			act 'Move away': gt 'obkitchen'
			end
		end
	end
