if $ARGS[0] = 'dress':
	cla
	'<center><img src="images/clothes/dress<<numdres>>.jpg "></center>'
	'(strength <<dressH[numdres]>>, hip size <<dressB[numdres]>>)'

	if mag = 1:
		'Price <<payclo>>'

		act 'Remove item.':
			gt $loc, $metka
		end

		if dress[numdres] = 0 and money >= payclo:
			act 'Buy':
				money -= payclo
				dress[numdres] = 1
				dressH[numdres] = 160
				dressB[numdres] = bedra
				gt $loc, $metka
			end
		end
	end

	if mag = 0:
		act 'In Closet':
			gt 'loker','start'
		end

		if dressH[numdres] > 0 and dressB[numdres] >= bedra - 8 and dressB[numdres] <= bedra + 8:
			act 'Wear':
				set numdress = numdres
				set odetjeans = 0
				set odetyoga = 0
				set odetsarafan = 0
				set odetshort = 0
				set odetskirt = 0
				set odetdress = 1
				set odetprofi = 0
				set odetpants = 0
				set odetlatex = 0
				set odethooker = 0
				set odetkis = 0
				set odetnewclo = 0
				set odetero = 0
				set vatnik = 0
				set glamur = 1
				set sport = 0
				set ski = 0
				set hook = 0
				set ofclo = 0
				set sexual = 0
				if cumfrot > 0: set cumfrot = 0
				set nude = 0
				set odevnesh = 15
				gt 'loker','start'
			end
		end

		if dressH[numdres] <= 0:
			'This item is worn and is not suitable for further wear.'
			set dress[numdres] = 0
			set dressH[numdres] = 0
			set nude = 1

			act 'Select another item':
				gt 'loker','start'
			end
		end

		if dressB[numdres] < bedra - 8 or dressB[numdres] > bedra + 8:
			'This thing is you no longer have the right size and not suitable for further wear.'

			act 'Select another item':
				gt 'loker','start'
			end
		end
	end
end

